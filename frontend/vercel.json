{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://genvest-production-ec2c.up.railway.app"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}

  